{
  "presets": [
    "es2017",
    "babel-preset-expo",
    "babel-preset-react",
    "stage-0"
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    },
    "test": {
      "presets": [
        "es2017",
        "babel-preset-expo",
        "babel-preset-react",
        "stage-0"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
